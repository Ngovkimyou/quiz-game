		"svelte": "^5.53.5",
		"svelte-check": "^4.3.6",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.2",
		"wrangler": "^4.63.0"
	},
